<table id="sample-table-1" class="table table-striped table-bordered table-hover">
<thead><tr><th class="hidden-480">送货日期</th><th>项目名称</th><th>销售金额</th><th>销售重量</th></tr></thead>
<tbody>
<% @profit.each do |f|%>
<tr>
<td class="hidden-480"><%=f.time%></td>
<td>
<%=_tip_item f.item,search_project_search_index_url(:item_id=>f.item.id)%>
</td>
<td><%=f.price%>元</td>
<td><%=f.weight%>吨</td>
</tr>
<% end %>
</tbody>
</table>